{
  "name": "ovsx-alternate-file",
  "displayName": "Open VSX Alternate File",
  "description": "Quickly open alternate files using projections.json (inspired by Rails and test alternates)",
  "version": "0.0.1",
  "publisher": "your-publisher-id",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:alternateFile.open"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "alternateFile.open",
        "title": "Open Alternate File"
      }
    ]
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/vscode": "^1.99.1",
    "typescript": "^5.0.0"
  }
}
